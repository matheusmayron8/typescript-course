{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API desenvolvida utilizando TS, NODE, ES6, Postgres e Sequelize",
  "main": "server/server.ts",
  "scripts": {
    "start": "PORT=5000 NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "PORT=5000 NODE_ENV=production node ./dist/server/server.js",
    "unit-test": "NODE_ENV=test mocha --opts dist/tests/unit/config/mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "NODE_ENV=test mocha --opts dist/tests/integration/config/mocha.opts ./dist/tests/integration/integration.test.js",
    "integration-coverage": "nyc --reporter=html npm run integration-test",
    "unit-coverage": "nyc --reporter=html npm run unit-test",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Node",
    "ES6"
  ],
  "author": "Matheus Mayron",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.7.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "http": "0.0.0",
    "http-status": "^1.4.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.14.0",
    "sequelize-cli": "^5.5.1",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "^3.5.29",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.5",
    "@types/express": "^4.17.2",
    "@types/http-status": "^1.1.2",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.37",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/sequelize": "^4.28.7",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "sequelize": "^5.21.2",
    "supertest": "^4.0.2",
    "testdouble": "^3.12.4",
    "ts-node": "^8.5.2"
  }
}
